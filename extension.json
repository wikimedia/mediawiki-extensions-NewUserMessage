{
	"name": "NewUserMessage",
	"version": "3.6.0, 2016-10-31",
	"author": [
		"[https://www.organicdesign.co.nz/User:Nad Aran Dunkley]",
		"Siebrand Mazeland"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:NewUserMessage",
	"descriptionmsg": "newusermessage-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.42"
	},
	"MessagesDirs": {
