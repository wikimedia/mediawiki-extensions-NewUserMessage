	},
	"HookHandlers": {
		"main": {
			"class": "MediaWiki\\Extension\\NewUserMessage\\NewUserMessage"
		}
	},
	"config": {
		"NewUserSuppressRC": {
			"value": false
		},
		"NewUserMinorEdit": {
			"value": true
		},
		"NewUserMessageOnAutoCreate": {
			"value": false
		}
